<%if @project.errors.any? %>
  $('#error-name').html('name <%= @project.errors["name"][0]%>!!');
  $('#error-ranking').html('ranking <%= @project.errors["ranking"][0]%>!!');
  $('#error-details').html('details <%= @project.errors["details"][0]%>!!');
<%else%>
  $('.parent').html('<%=j render "welcome/projects"%>');
  $('#modalLoginForm').modal('hide');
  $('#project_close').click();  
  $('#error-name').html('');
  $('#error-ranking').html('');
  $('#error-details').html('');
  
  document.getElementById("my-form").reset();
<%end%>
